# ADK Debug Mode
ADK_DEBUG=false

# Google/Gemini API Key
GOOGLE_API_KEY=your_google_api_key_here

# LLM Model
LLM_MODEL=gemini-2.5-flash

# Etherscan API Key
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Payment Wallet Configuration (for x402 server)
# Private key of the wallet that receives payments
PAYMENT_PRIVATE_KEY=0x_your_private_key_here

# Public address where payments are sent
PAYMENT_WALLET=0x_your_wallet_address_here

# Next.js Public Variables
# API URL for x402 payment server
NEXT_PUBLIC_API_URL=http://localhost:3001
